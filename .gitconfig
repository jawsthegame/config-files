[user]
  name = Tom
  email = tom@vistarmedia.com
[core]
  editor = vim
  excludesfile = /home/tom/.gitignore_global
[color]
  ui = true
[alias]
  st = status
  pod = pull origin develop
  prefs = push origin HEAD:refs/for/develop
  prefsm = push origin HEAD:refs/for/master
  lol = log --oneline
  pom = pull origin master
  pushd = push origin develop
  pushm = push origin master
  co = checkout
  cod = checkout develop
  com = checkout master
  fall = fetch --all
  pusht = push --tags
  graph = log --all --graph --decorate --oneline
